generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["fullTextSearchPostgres"]
}
				
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
												
model Obra {
  id           Int    @id @default(autoincrement())
  titulo       String @unique
  imagen       String
  descripcion  String
  procedencia  String
  comentario   String
}
